<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sample.asm2.controller.GroupController"
            prefWidth="600" prefHeight="400">
    <children>
        <Label alignment="CENTER" layoutX="19.0" layoutY="10.0" prefHeight="25.0" prefWidth="147.0" text="Enter group name" />
        <TextField fx:id="search" layoutX="166.0" layoutY="10.0" prefHeight="25.0" prefWidth="279.0" />
        <Button fx:id="btnSearch" layoutX="460.0" layoutY="10.0" mnemonicParsing="false" text="Search" onAction="#groupAction"/>
        <ListView fx:id="tblGroup" layoutX="21.0" layoutY="60.0" prefHeight="180.0" prefWidth="272.0"/>
        <Label layoutX="349.0" layoutY="77.0" text="Enter group name">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
        <TextField fx:id="groupName" layoutX="349.0" layoutY="128.0" prefHeight="25.0" prefWidth="227.0" text="" />
        <Button fx:id="btnAdd" layoutX="350.0" layoutY="200.0" mnemonicParsing="false" text="Save" onAction="#groupAction"/>
        <Button fx:id="btnUpdate" layoutX="436.0" layoutY="200.0" mnemonicParsing="false" text="Update" onAction="#groupAction"/>
        <Button fx:id="btnDelete" layoutX="525.0" layoutY="200.0" mnemonicParsing="false" text="Delete" onAction="#groupAction"/>
        <Button fx:id="btnClose" layoutX="524.0" layoutY="354.0" mnemonicParsing="false" text="Close" onAction="#groupAction"/>
    </children>
</AnchorPane>
